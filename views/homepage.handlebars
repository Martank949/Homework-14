<div class="container">
  <div class="card-header">
    <h2>
      Daily Mountain Bike Blogs
    </h2>
  </div>
  <!-- going to pass in an array of objects from "blogs" -->
  {{#each blogs as |blog|}}
  <div class="card">
      <h5 class="card-header">#{{blog.id}} - Posted By: {{blog.user_name}}</h5>
        <div class="card-body">
          <h5 class="card-title">{{blog.blog_name}}</h5>
      <p class="card-text">{{blog.description}}</p>
    </div>
  </div>
    {{/each}}
